#!/bin/bash

#SBATCH -t 12:00:00
#SBATCH -p hopper

# Adapt according to your installation.
CONDA_BASE_DIR=~/software/miniconda3

# Activate the conda environment.
source "$CONDA_BASE_DIR/etc/profile.d/conda.sh"

source activate Torch-CUDA-11

# for moses2
# 1 = --run_name unconditional_moses
# 2 = --data_name moses2

echo $1
echo $2

python train/train.py --run_name $1 --data_name $2 --batch_size 384 --max_epochs 10 --num_props 0
